                    '{"MgType":"SMG_thunderstruckWildLightning"}': '雷霆萬鈞之百搭閃電',
                    '{"MgType":"SMG_treasuresOfLionCity"}': '海底寶城',
                    '{"MgType":"SMG_wolfBlazeMegaways"}': '烈焰戰狼',
                    '{"MgType":"SMG_leprechaunStrike"}': '妖精出擊',
                    '{"MgType":"SMG_candyRushWilds"}': '歡樂糖果百搭',
                    '{"MgType":"SMG_luckyLeprechaunClusters"}': '幸運矮妖精集群獎金',
                    '{"MgType":"SMG_maskOfAmun"}': '阿蒙的面具',
                    '{"MgType":"SMG_777Surge"}': '777電光火石',
                    '{"MgType":"SMG_luckyClucks"}': '幸運雞',
                    '{"MgType":"SFG_WDGoldenFuwaFishing"}': '萬達金福娃釣魚',
                    '{"MgType":"SMG_777RoyalWheel"}': '777皇家幸運輪',
                    '{"MgType":"SMG_basketballStarDeluxe"}': '籃球巨星豪華版',
                    '{"MgType":"SMG_candyRushWilds2"}': '歡樂糖果百搭2',
                    '{"MgType":"SMG_crazyBobBonanza"}': '瘋狂海底大寶箱',
                },

                #  BBIN
                'bbin': {
                    '{"BbinType":"62","gameCode":"5275"}': '麻將胡了',
                    '{"BbinType":"62","gameCode":"5067"}': '大話西遊',
                    '{"BbinType":"62","gameCode":"5256"}': '爆爆生財',
                    '{"BbinType":"62","gameCode":"5166"}': '葫蘆娃',
                    '{"BbinType":"62","gameCode":"5259"}': '爆利金剛',
                    '{"BbinType":"62","gameCode":"5311"}': '翻轉神廟',
                    '{"BbinType":"62","gameCode":"5198"}': '下龍灣神話',
                    '{"BbinType":"62","gameCode":"5098"}': '五福臨門',
                    '{"BbinType":"62","gameCode":"5267"}': 'AV派對',
                    '{"BbinType":"62","gameCode":"5273"}': 'Hello Tiki',
                    '{"BbinType":"62","gameCode":"5094"}': '金瓶梅2',
                },

                #  TP
                'tp': {
                    '{"tp_gameCode":"ds_3079"}': '福運雙星',
                    '{"tp_gameCode":"at2_063"}': '財神麻將',
                    '{"tp_gameCode":"ds_3030"}': '金猴爺',
                },
            }

            # 用户输入
            游戏平台_中文 = self.yxpt
            游戏名称_中文 = self.yxmc

            # 检查输入是否有效
            if 游戏平台_中文 not in platform_mapping.values():
                self.signals.finished.emit(f"错误：未知的游戏平台 '{游戏平台_中文}'")

            # 获取平台代码
            游戏平台_代码 = [k for k, v in platform_mapping.items() if v == 游戏平台_中文][0]

            # 检查游戏名称是否在该平台中存在
            if 游戏平台_代码 not in game_type_mapping or 游戏名称_中文 not in game_type_mapping[游戏平台_代码].values():
                self.signals.finished.emit(f"错误：在平台 '{游戏平台_中文}' 中找不到游戏名称 '{游戏名称_中文}'")

            # 获取游戏类型代码
            游戏名称_代码 = [k for k, v in game_type_mapping[游戏平台_代码].items() if v == 游戏名称_中文][0]

            print(f'游戏平台代码: {游戏平台_代码}')
            print(f'游戏名称代码: {游戏名称_代码}')

            headers = {
                'Cookie': f'token={self.token}',
            }

            url = f"http://v.boinht.com:{self.port}/pp_game_log/pf_list"

            wb = Workbook()
            ws = wb.active
            ws.append(
                ['编号ID', '游戏ID', '游戏平台', '单号', '游戏名称', '操作时间', '下注总金额', '中奖金额', '有效下注',
                 '小费', '取消下注'])

            page = 1
            total_pages = 0

            while True:
                # self.signals.finished.emit(f"正在请求第 {page} 页数据...")  # 实时显示页数
                print(f"正在请求第 {page} 页数据...")

                data = {
                    "channel_id": "",
                    "open_channel_id": "",
                    "pid": 游戏平台_代码,
                    "game_type": 游戏名称_代码,
                    "user_id": "",
                    "transfer_id": "",
                    "begin_time": self.kssj,
                    "end_time": self.jssj,
                    "page_size": 100000,
                    "page": page,
                    "channel_i": "",
                    "channel_ii": "",
                    "login_code": self.code,
                }

                try:
                    response = requests.post(url, data=data, headers=headers, timeout=20)
                    response.raise_for_status()
                    result_list = response.json().get('result', [])
                except requests.exceptions.RequestException as e:
                    print(f"请求失败: {e}")
                    self.signals.finished.emit(f"请求失败: {e}")
                    self.signals.enable_buttons.emit()
                    return

                if not result_list:
                    break  # 如果这一页没数据了，结束循环

                total_pages += 1

                for aa in result_list:
                    # 平台映射
                    游戏平台_显示 = platform_mapping.get(aa['pid'], aa['pid'])

                    # 游戏名称映射
                    if aa['pid'] in game_type_mapping and aa['game_type'] in game_type_mapping[aa['pid']]:
                        游戏名称_显示 = game_type_mapping[aa['pid']][aa['game_type']]
                    else:
                        游戏名称_显示 = aa['game_type']

                    编号ID = aa['id']
                    游戏ID = aa['user_id']
                    单号 = aa['transfer_id']
                    操作 = aa['game_time']
                    操作时间 = datetime.fromtimestamp(操作).strftime('%Y-%m-%d %H:%M:%S')
                    下注总金额 = aa['bet']
                    中奖金额 = aa['win']
                    有效下注 = aa['available_bet']
                    小费 = aa['tip']
                    取消下注 = aa['op_type']
                    if 取消下注 in (0, 13):
                        取消下注 = '否'

                    ws.append(
                        [编号ID, 游戏ID, 游戏平台_显示, 单号, 游戏名称_显示, 操作时间, 下注总金额, 中奖金额, 有效下注,
                         小费, 取消下注])

                page += 1  # 翻到下一页

            # 保存并打开Excel
            rq = datetime.now().strftime('%Y年%m月%d日%H时%M分%S秒')
            file_name = f'导出数据_{rq}.xlsx'
            wb.save(file_name)
            os.startfile(file_name)

            # self.signals.finished.emit("数据处理完成，Excel 文件已打开！")
            self.signals.enable_buttons.emit()

        except Exception as e:
            # self.signals.finished.emit(f"发生错误: {e}")
            self.signals.enable_buttons.emit()  # 即使发生错误，也要启用按钮


class UserWindow(QWidget):
    def __init__(self):
        super().__init__()
        # 🌐 联网验证 + 时间验证
        self.check_network_validation()

        self.resize(700, 500)  # 初始大小
        self.setWindowTitle("获取指定游戏数据")

        # 加载 base64 图标（确保字符串完整）
        icon_base64 = ""  # 替换成你的 base64
        icon_data = QByteArray.fromBase64(icon_base64.encode())

        # 更可靠的图标加载方式
        pixmap = QPixmap()
        if not pixmap.loadFromData(icon_data, "ICO"):  # 显式指定格式
            print("❌ 图标加载失败！请检查 base64 数据或文件格式")
        else:
            self.setWindowIcon(QIcon(pixmap))  # 设置窗口和任务栏图标

        layout = QHBoxLayout()  # 横向排列！

        self.setup_config_group(layout)  # 左侧：配置区
        self.setup_data_group(layout)  # 右侧：数据操作区

        self.setLayout(layout)

        self.thread_pool = QThreadPool()
        self.load_config()
        self.recharge_data = []
        self.workers = []

    def check_network_validation(self):
        """联网验证是否允许运行（隐蔽版）"""
        try:
            program_id = "program_chaozhidingyouxi"  # 每个程序用自己的名字（必须和JSON文件里的 key 对应）
            response = requests.get("http://8.210.92.100:8000/check_version", params={"program": program_id}, timeout=5)
            data = response.json()

            status = data.get("status")

            if status != "active":
                QMessageBox.critical(self, "兼容性错误", "系统组件加载失败（Code: S-1）")
                sys.exit(0)

        except Exception:
            QMessageBox.critical(self, "兼容性错误", "组件连接失败，请稍后重试（Code: S-2）")
            sys.exit(0)

    def show_floating_message(self, message, duration=2000):
        label = QLabel(message, self)
        label.setAlignment(Qt.AlignCenter)
        label.setStyleSheet("""
            QLabel {
                background-color: #d1f0e1;     /* 背景颜色：浅绿色，提示框的底色 */
                color: #1d5c49;               /* 字体颜色：深绿色，内容文字的颜色 */
                font-size: 16px;              /* 字体大小：16 像素，适中清晰 */
                border: 1px solid #a0c5b1;    /* 边框样式：1像素实线，颜色为淡绿色 */
                border-radius: 8px;           /* 边角圆润度：边角圆弧半径 8 像素 */
                padding: 10px;                /* 内边距：四边留出10像素，让文字不紧贴边框 */
            }
        """)
        label.resize(380, 50)

        # 居中显示
        parent_width = self.frameGeometry().width()
        parent_height = self.frameGeometry().height()
        label.move((parent_width - label.width()) // 2, (parent_height - label.height()) // 2)
        label.show()

        QTimer.singleShot(duration, label.deleteLater)

    def setup_data_group(self, parent_layout):
        group = QGroupBox("功能操作")
        layout = QVBoxLayout()

        game_info_layout = QHBoxLayout()
        self.srk_yxpt = QLineEdit(self)
        self.srk_yxpt.setPlaceholderText("请输入游戏平台")
        self.srk_yxpt.setFixedHeight(40)
        game_info_layout.addWidget(self.srk_yxpt)

        self.srk_yxmc = QLineEdit(self)
        self.srk_yxmc.setPlaceholderText("请输入游戏名称")
        self.srk_yxmc.setFixedHeight(40)
        game_info_layout.addWidget(self.srk_yxmc)
        layout.addLayout(game_info_layout)

        self.srk_kssj = QDateTimeEdit(self)
        self.srk_kssj.setCalendarPopup(True)
        self.srk_kssj.setDisplayFormat("yyyy-MM-dd HH:mm:ss")
        self.srk_kssj.wheelEvent = lambda event: event.ignore()  # 禁用滚轮
        self.srk_kssj.setFixedHeight(40)
        layout.addWidget(self.srk_kssj)

        self.srk_jssj = QDateTimeEdit(self)
        self.srk_jssj.setCalendarPopup(True)
        self.srk_jssj.setDisplayFormat("yyyy-MM-dd HH:mm:ss")
        self.srk_jssj.wheelEvent = lambda event: event.ignore()  # 禁用滚轮
        self.srk_jssj.setFixedHeight(40)
        layout.addWidget(self.srk_jssj)

        current_datetime = datetime.now()
        self.srk_kssj.setDate(current_datetime.date())
        end_datetime = current_datetime.replace(hour=23, minute=59, second=59)
        self.srk_jssj.setDateTime(end_datetime)

        self.get_button = QPushButton("🚀 获取数据")
        self.get_button.clicked.connect(self.start_processing)
        layout.addWidget(self.get_button)

        group.setLayout(layout)
        parent_layout.addWidget(group)

    def setup_config_group(self, parent_layout):
        group = QGroupBox("参数配置")
        layout = QVBoxLayout()

        # cookie
        token_layout = QHBoxLayout()
        self.srk_token = QLineEdit(self)
        self.srk_token.setPlaceholderText("请输入 Cookie")
        self.srk_token.setFixedHeight(40)
        token_layout.addWidget(self.srk_token)
        layout.addLayout(token_layout)

        # port
        port_layout = QHBoxLayout()
        self.srk_port = QLineEdit(self)
        self.srk_port.setPlaceholderText("请输入端口号,例如 8080")
        self.srk_port.setFixedHeight(40)
        port_layout.addWidget(self.srk_port)
        layout.addLayout(port_layout)

        # code
        code_layout = QHBoxLayout()
        self.srk_code = QLineEdit(self)
        self.srk_code.setPlaceholderText("请输入小明计算器登录码")
        self.srk_code.setFixedHeight(40)
        code_layout.addWidget(self.srk_code)
        layout.addLayout(code_layout)

        self.save_button = QPushButton("💾 保存配置")
        self.save_button.clicked.connect(self.manual_save)
        layout.addWidget(self.save_button)

        group.setLayout(layout)
        parent_layout.addWidget(group)

        self.setStyleSheet("""
            QWidget { font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; }  /* 设置全局字体 */

            QLineEdit {
                height: 18px;  /* 输入框高度 */
                font-size: 14px;  /* 输入框字体大小 */
                padding: 4px 8px;  /* 输入框内边距 */
                border: 1.5px solid #cccccc;  /* 输入框边框颜色和宽度 */
                border-radius: 6px;  /* 输入框圆角 */
                background-color: #ffffff;  /* 输入框背景色 */
            }

            QDateTimeEdit {
                font-size: 20px;          /* 日期框字体大小 */
            }

            QLineEdit:focus {
                border-color: #4a90e2;  /* 输入框聚焦时边框颜色 */
                background-color: #eaf4ff;  /* 输入框聚焦时背景色 */
            }

            QPushButton {
                height: 36px;  /* 按钮高度 */
                font-size: 15px;  /* 按钮字体大小 */
                color: white;  /* 按钮文字颜色 */
                background-color: #4a90e2;  /* 按钮背景颜色 */
                border: none;  /* 按钮无边框 */
                border-radius: 8px;  /* 按钮圆角 */
                font-weight: bold;  /* 按钮文字加粗 */
                margin-top: 10px;  /* 按钮顶部外边距 */
            }

            QPushButton:hover {
                background-color: #357ABD;  /* 鼠标悬停时按钮背景颜色 */
            }

            QPushButton:pressed {
                background-color: #2C5AA0;  /* 按钮按下时背景颜色 */
            }

            QPushButton:disabled {
                background-color: #eee;  /* 禁用按钮背景色 */
                border: 1px solid #aaa;  /* 禁用按钮边框 */
                color: #999;  /* 禁用按钮文字颜色 */
            }

            QGroupBox {
                border: 1px solid #aaa;  /* 组框边框颜色和宽度 */
                border-radius: 8px;  /* 组框圆角 */
                margin-top: 10px;  /* 组框顶部外边距 */
            }

            QGroupBox::title {
                subcontrol-origin: margin;  /* 组框标题基于边距定位 */
                subcontrol-position: top center;  /* 组框标题居中 */
                padding: 0 6px;  /* 组框标题内边距 */
                font-size: 15px;  /* 组框标题字体大小 */
                font-weight: bold;  /* 组框标题加粗 */
            }

            QMessageBox { 
                font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;  /* 弹窗字体 */
                font-size: 13px;  /* 弹窗字体大小 */
            }

            QMessageBox QPushButton {
                min-width: 80px;  /* 弹窗按钮最小宽度 */
                min-height: 28px;  /* 弹窗按钮最小高度 */
                border-radius: 6px;  /* 弹窗按钮圆角 */
                font-size: 13px;  /* 弹窗按钮字体大小 */
                background-color: #4a90e2;  /* 弹窗按钮背景颜色 */
                color: white;  /* 弹窗按钮文字颜色 */
            }

            QMessageBox QPushButton:hover {
                background-color: #357ABD;  /* 弹窗按钮悬停背景颜色 */
            }

            QMessageBox QPushButton:pressed {
                background-color: #2C5AA0;  /* 弹窗按钮按下背景颜色 */
            }
        """)

    def start_processing(self):
        try:
            port = self.srk_port.text()
            token = self.srk_token.text()
            code = self.srk_code.text()

            yxpt = self.srk_yxpt.text().strip()
            yxmc = self.srk_yxmc.text().strip()
            print(f"正在查询...游戏平台: {yxpt}, 游戏名称: {yxmc}")

            ks = self.srk_kssj.dateTime().toString("yyyy-MM-dd HH:mm:ss")
            js = self.srk_jssj.dateTime().toString("yyyy-MM-dd HH:mm:ss")

            kssjj = datetime.strptime(ks, '%Y-%m-%d %H:%M:%S')
            jssjj = datetime.strptime(js, '%Y-%m-%d %H:%M:%S')
            print(f"查询时间: {kssjj} 到 {jssjj}")

            kssj = int(kssjj.timestamp())
            jssj = int(jssjj.timestamp())

            signals = WorkerSignals()

            worker = DataWorker(port, token, code, kssj, jssj, yxpt, yxmc, signals)
            self.workers.append(worker)
            signals.finished.connect(self.on_task_finished)
            signals.enable_buttons.connect(self.enable_buttons)

            self.thread_pool.start(worker)

            self.show_floating_message("后台正在获取数据，请稍候...", duration=2500)

            self.srk_port.setEnabled(False)
            self.srk_token.setEnabled(False)
            self.srk_code.setEnabled(False)
            self.save_button.setEnabled(False)

            self.srk_yxpt.setEnabled(False)
            self.srk_yxmc.setEnabled(False)
            self.srk_kssj.setEnabled(False)
            self.srk_jssj.setEnabled(False)
            self.get_button.setEnabled(False)

        except Exception as e:
            print(f"发生错误: {e}")

    def on_task_finished(self, message):
        QMessageBox.information(self, "提示", message)

    def enable_buttons(self):
        self.srk_port.setEnabled(True)
        self.srk_token.setEnabled(True)
        self.srk_code.setEnabled(True)
        self.save_button.setEnabled(True)


        self.srk_yxpt.setEnabled(True)
        self.srk_yxmc.setEnabled(True)
        self.srk_kssj.setEnabled(True)
        self.srk_jssj.setEnabled(True)
        self.get_button.setEnabled(True)

    def save_config(self, port, token, code):
        config = ConfigParser()
        config['USER'] = {'Port': port, 'Token': token, 'Code': code}
        with open('config.ini', 'w') as configfile:
            config.write(configfile)

    def load_config(self):
        config = ConfigParser()
        config.read('config.ini')
        if 'USER' in config:
            self.srk_port.setText(config['USER'].get('Port', ''))
            self.srk_token.setText(config['USER'].get('Token', ''))
            self.srk_code.setText(config['USER'].get('Code', ''))

    def manual_save(self):
        port = self.srk_port.text()
        token = self.srk_token.text()
        code = self.srk_code.text()
        if port and token and code:
            self.save_config(port, token, code)
            QMessageBox.information(self, "提示", "配置已保存！")
        else:
            QMessageBox.warning(self, "警告", "请填写完整的配置！")


if __name__ == "__main__":
    app = QApplication(sys.argv)
    window = UserWindow()
    window.show()
    sys.exit(app.exec_())
