# 创建一个信号发送对象类
class WorkerSignals(QObject):
    finished = pyqtSignal(str)  # 任务完成信号
    enable_buttons = pyqtSignal()  # 新增信号：通知 UI 重新启用按钮


# 子线程工作类
class DataWorker(QRunnable):
    def __init__(self, port, token, code, kssj, jssj, yxpt, yxmc, signals):
        super().__init__()
